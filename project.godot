config_version=5

[application]

config/name="Global XI Football Maestro"
config/features=PackedStringArray("4.2", "GL Compatibility")

[autoload]

GameMapManager="*res://Resources/GameMap/GameMapManager1.gd"
UnicodeNormalizer="*res://addons/goutte.unicode/singleton/UnicodeNormalizer.gd"
LogDuck="*res://addons/LogDuck/LogDuck.gd"
uuid="*res://Singletons/uuid.gd"
GameSettings="*res://Resources/Settings/Settings.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[debug]

file_logging/enable_file_logging=true
file_logging/max_log_files=10
settings/stdout/verbose_stdout=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"
window/icon=""

[editor_plugins]

enabled=PackedStringArray("res://addons/LogDuck/plugin.cfg", "res://addons/goutte.unicode/plugin.cfg", "res://addons/gut/plugin.cfg", "res://addons/input_controller/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

